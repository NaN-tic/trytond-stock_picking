<form string="Done Picking Shipment Out" col="6">
    <label name="shipment"/>
    <field name="shipment"/>
    <label name="number_packages"/>
    <field name="number_packages"/>
    <notebook colspan="6">
        <page string="Products" id="product">
            <field name="lines" colspan="6"/>
        </page>
        <page string="Notes" id="notes" states="{'invisible': ~Eval('note')}">
            <field name="note" colspan="6"/>
        </page>
    </notebook>
</form>
